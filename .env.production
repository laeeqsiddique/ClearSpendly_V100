# Production Environment Configuration for ClearSpendly
NODE_ENV=production

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-production-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_production_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_production_service_role_key

# AI-Enhanced OCR Configuration (Production optimized)
NEXT_PUBLIC_OPENAI_API_KEY=your_production_openai_api_key
NEXT_PUBLIC_AI_MODEL=gpt-4o-mini
NEXT_PUBLIC_ENABLE_AI_ENHANCEMENT=true
NEXT_PUBLIC_AI_CONFIDENCE_THRESHOLD=95

# Production AI Configuration with fallback
ENABLE_AI_OCR_ENHANCEMENT=true
LLM_PROVIDER=openai
LLM_MODEL=gpt-4o-mini
LLM_FALLBACK_PROVIDER=ollama
LLM_FALLBACK_MODEL=llama3.2:3b
LLM_API_URL=https://api.openai.com/v1
LLM_TEMPERATURE=0.05
LLM_MAX_TOKENS=2000
LLM_TIMEOUT_MS=60000

# Database Configuration
DATABASE_URL=postgresql://postgres:your_production_password@your-production-db.supabase.co:5432/postgres

# External Services (Production)
RESEND_API_KEY=re_fG1Lj8ZX_FCgyq14uq8btpE9VJAB3rexk
STRIPE_SECRET_KEY=sk_live_production_key
STRIPE_WEBHOOK_SECRET=whsec_production_webhook_secret

# Storage Configuration (Production)
AWS_ACCESS_KEY_ID=your_production_aws_access_key
AWS_SECRET_ACCESS_KEY=your_production_aws_secret_key
AWS_REGION=us-east-1
AWS_S3_BUCKET=clearspendly-production-uploads
AWS_CLOUDFRONT_DOMAIN=d1234567890.cloudfront.net

# OAuth Configuration (Production)
GOOGLE_CLIENT_ID=your_production_google_client_id
GOOGLE_CLIENT_SECRET=your_production_google_client_secret
AZURE_CLIENT_ID=your_production_azure_client_id
AZURE_CLIENT_SECRET=your_production_azure_client_secret

# Monitoring and Analytics
NEXT_PUBLIC_POSTHOG_KEY=your_production_posthog_key
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com
DATADOG_API_KEY=your_datadog_api_key
NEW_RELIC_LICENSE_KEY=your_newrelic_license_key

# Application Configuration
NEXT_PUBLIC_APP_URL=https://app.clearspendly.com
NEXT_PUBLIC_API_URL=https://app.clearspendly.com/api

# Feature Flags (Production)
NEXT_PUBLIC_FEATURE_AI_OCR=true
NEXT_PUBLIC_FEATURE_MULTI_TENANT=true
NEXT_PUBLIC_FEATURE_ANALYTICS=true
NEXT_PUBLIC_FEATURE_INVOICE_GENERATION=true
NEXT_PUBLIC_FEATURE_ADVANCED_REPORTING=true

# Security Configuration (Production hardened)
NEXTAUTH_URL=https://app.clearspendly.com
NEXTAUTH_SECRET=your_production_nextauth_secret_very_long_and_secure
ENCRYPTION_KEY=your_production_encryption_key_32_chars_long

# Rate Limiting (Production)
RATE_LIMIT_REQUESTS_PER_MINUTE=60
RATE_LIMIT_REQUESTS_PER_HOUR=1000
RATE_LIMIT_BURST_SIZE=10

# Logging (Production)
LOG_LEVEL=info
ENABLE_REQUEST_LOGGING=false
LOG_RETENTION_DAYS=30

# Performance Configuration
NEXT_PUBLIC_CDN_URL=https://cdn.clearspendly.com
REDIS_URL=redis://your-production-redis:6379
CACHE_TTL_SECONDS=3600

# Backup Configuration (Production)
BACKUP_RETENTION_DAYS=30
AUTO_BACKUP_ENABLED=true
BACKUP_SCHEDULE="0 1 * * *"  # Daily at 1 AM
BACKUP_ENCRYPTION_ENABLED=true

# Compliance and Security
GDPR_COMPLIANCE_ENABLED=true
SOC2_COMPLIANCE_ENABLED=true
DATA_RETENTION_DAYS=2555  # 7 years
AUDIT_LOGGING_ENABLED=true

# Multi-tenant Configuration
MAX_TENANTS_PER_ORGANIZATION=50
TENANT_ISOLATION_LEVEL=strict
CROSS_TENANT_ACCESS_LOGGING=true

# Auto-scaling Configuration
MIN_INSTANCES=2
MAX_INSTANCES=10
TARGET_CPU_UTILIZATION=70
TARGET_MEMORY_UTILIZATION=80

# Health Check Configuration
HEALTH_CHECK_INTERVAL=30
HEALTH_CHECK_TIMEOUT=10
HEALTH_CHECK_RETRIES=3RAILWAY_BUILD_TIME=Wed, Jul 30, 2025 12:28:48 AM
